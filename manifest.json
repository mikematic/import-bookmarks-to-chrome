{
  "manifest_version": 2,

  "name": "My Bookmark Manager",
  "description": "My Bookmark Manager to organize my bookmarks",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
     "default_title": "Click here!"
  },
  "permissions": [
    "bookmarks"
  ]
}
