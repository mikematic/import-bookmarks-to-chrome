{
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' https://ajax.googleapis.com; object-src 'self'",
  "name": "Import Bookmarks to Chrome",
  "description": "This extension allows you to import your bookmarks into chrome bookmarks",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
     "default_title": "Click here!"
  },
  "permissions": [
    "bookmarks"
  ]
}
